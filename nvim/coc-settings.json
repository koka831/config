{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "lua",
    "vim",
    "dart",
    "jsonc",
    "rust",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.enabledLanguageIds": [
    "dart",
    "go",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "markdown",
    "plaintext",
    "ruby",
    "rust",
    "typescript",
    "typescriptreact",
    "yml",
    "vim"
  ],
  "codeLens.enable": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.errorSign": ">>",
  "diagnostic.warningSign": ">>",
  "git.enableGutters": false,
  "eslint.autoFixOnSave": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.inlayHints.chainingHintsSeparator": "‣",
  "rust-analyzer.inlayHints.typeHintsSeparator": "‣",
  "rust-analyzer.updates.prompt": false,
  "snippets.ultisnips.directories": [
    "ultisnips",
    "ultisnips/gen"
  ],
  "flutter.sdk.dart-lookup": "fvm dart",
  "flutter.sdk.flutter-lookup": "fvm flutter",
  // run analyzer immediately on open folder
  "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
  "solargraph.commandPath": "/home/koka/.rbenv/shims/solargraph",
  "solargraph.checkGemVersion": true,
  "solargraph.transport": "stdio",
  "solargraph.completion": true,
  "solargraph.definitions": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "solargraph.references": true,
  "solargraph.symbols": true,
  "sql.database": "postgresql",
  "suggest.floatEnable": true,
  "suggest.enablePreview": true,
  "suggest.echodocSupport": true,
  "cSpell.userWords": [
    "Hokenbo",
    "MVVM",
    "Quickfix",
    "RGBO",
    "Riverpod",
    "clippy",
    "datetime",
    "deeplink",
    "dotenv",
    "gruvbox",
    "hoge",
    "koka",
    "postgresql",
    "pubspec",
    "repr",
    "reqwest",
    "struct",
    "vite"
  ]
}
